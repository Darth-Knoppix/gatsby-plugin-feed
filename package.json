{
  "name": "@darth-knoppix/gatsby-plugin-feed",
  "version": "0.0.0-development",
  "description": "A Gatsby plugin to generate RSS, JSON and Atom feeds using Feed by jpmonette",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Darth-Knoppix/gatsby-plugin-feed.git"
  },
  "scripts": {
    "commit": "npx git-cz",
    "build": "babel src --out-dir . --ignore '**/*.test.js'",
    "watch": "babel -w src --out-dir . --ignore '**/*.test.js'",
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "homepage": "https://github.com/Darth-Knoppix/gatsby-plugin-feed",
  "bugs": {
    "url": "https://github.com/Darth-Knoppix/gatsby-plugin-feed/issues"
  },
  "author": "Seth Corker <seth.corker@gmail.com>",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "rss",
    "atom",
    "jsonfeed",
    "feed"
  ],
  "license": "MIT",
  "dependencies": {
    "feed": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.0",
    "@babel/core": "^7.10.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-env": "^7.10.0",
    "cz-conventional-changelog": "3.2.0",
    "jest": "^26.0.1",
    "semantic-release": "^17.0.8"
  },
  "peerDependencies": {
    "gatsby": "^2.1.0"
  },
  "files": [
    "default-options.js",
    "gatsby-node.js",
    "gatsby-ssr.js",
    "index.js",
    "utils.js"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "master"
    ]
  }
}
